public class HikariCPDataSource {
	private static HikariConfig config = new HikariConfig();
	
	private static HikariDataSource ds;
	
	static {
		config.setDriverClassName(JDBCUtil.DRIVER_NAME);
		config.setJdbcUrl(null);
	}
}
